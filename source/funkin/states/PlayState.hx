		if (ret.contains(CoolVars.Function_Stop))
			return;

		var str:String = '';

		if(totalPlayed != 0)
		{
			var percent:Float = CoolUtil.floorDecimal(ratingPercent * 100, 2);

			str += '${percent}% - ${ratingFC}';
		}

		var tempScore:String = 'Score: ${songScore}'
		+ '    Misses: ${songMisses}'
		+ '    Rating: ' + (str.trim() == '' ? '?' : str);
		
